{
  "name": "flexhire-react",
  "version": "1.0.0",
  "repository": "https://github.com/flexhire/flexhire",
  "description": "FlexHire",
  "main": "index.js",
  "scripts": {
    "dev": "cross-env NODE_OPTIONS=--max_old_space_size=4096 next -- -p 8080",
    "dev-networked": "cross-env NODE_OPTIONS=--max_old_space_size=4096 next -- -p 8080 --hostname 0.0.0.0",
    "relay": "relay-compiler --language typescript --watchman false $@",
    "typecheck": "npm run typecheck-app",
    "typecheck-app": "tsc --noEmit -p .",
    "typecheck-cypress": "tsc --noEmit -p ./cypress",
    "prebuild": "npm run typecheck",
    "build": "cross-env NODE_OPTIONS=--max_old_space_size=4096 next build",
    "start": "next start -- -p 8080",
    "production": "cross-env NODE_OPTIONS=--max_old_space_size=4096 APP_ENV=production next build",
    "staging": "cross-env NODE_OPTIONS=--max_old_space_size=4096 APP_ENV=staging next build",
    "delete-source-maps": "find .next/ -type f -name \"*.map\" -delete",
    "eslint": "eslint --format 'node_modules/eslint-friendly-formatter' ./app",
    "test": "TZ='UTC' jest --watch",
    "test-once": "TZ='UTC' jest",
    "test-ci": "TZ='UTC' jest --ci --reporters=default --reporters=jest-junit -w 1",
    "test-update-snapshots": "TZ='UTC' jest -u",
    "clean:reports": "mkdir -p test-results/cypress/; rm test-results/cypress/* || true",
    "precypress:run": "npm run clean:reports",
    "cypress:install": "cypress install",
    "cypress:open-ct": "cypress open-ct",
    "cypress:open": "cypress open",
    "cypress:open:mobile": "cypress open --config viewportHeight=736,viewportWidth=414 --env RUN_RESOLUTION=MOBILE",
    "cypress:run-ct": "cypress run-ct --reporter mocha-multi-reporters --reporter-options configFile=reporter-config.json",
    "cypress:run": "cypress run --reporter mocha-multi-reporters --reporter-options configFile=reporter-config.json",
    "cypress:run:mobile": "cypress run --reporter mocha-multi-reporters --reporter-options configFile=reporter-config.json --config viewportHeight=736,viewportWidth=414",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "build-embed-scripts": "cross-env webpack --config ./webpack.config.js",
    "build-embed-scripts-staging": "cross-env APP_ENV=staging webpack --config ./webpack.config.js",
    "build-embed-scripts-production": "cross-env APP_ENV=production webpack --config ./webpack.config.js"
  },
