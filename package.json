  "jest": {
    "testMatch": [
      "**/*.test.(js|ts|tsx|jsx)"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/app/setupJest.js"
    ],
    "modulePaths": [
      "<rootDir>/app",
      "<rootDir>"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "<rootDir>/docker"
    ],
    "testEnvironment": "jsdom",
    "moduleNameMapper": {
      "^.+\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)(\\?[a-z]*)?$": "<rootDir>/app/__mocks__/fileMock.js",
      "^.+\\.(css|less)$": "identity-obj-proxy"
    },
    "setupFiles": [
      "raf/polyfill"
    ]
  },
  "sideEffects": [
    "**/*.css",
    "./pages/_app.tsx",
    "./pages/_document.tsx"
  ]
