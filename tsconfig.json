{
  "compilerOptions": {
    "allowJs": true,
    "jsx": "preserve",
    "esModuleInterop": true,
    "noImplicitAny": false,
    "baseUrl": ".",
    "paths": {
      "*": [
        "app/*",
        "*"
      ]
    },
    "target": "es5",
    "lib": [
      "es5",
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "skipLibCheck": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noUnusedLocals": false,
    "noUnusedParameters": false
  },
  "exclude": [
    "node_modules",
    "cypress"
  ],
  "include": [
    "next-env.d.ts",
    "app/types/css.d.ts",
    "**/*.ts",
    "**/*.tsx",
    "**/*.jsx",
    "**/*.js"
  ]
}
